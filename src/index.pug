extends modules/layout/_layout

prepend head
	title Клининг
	meta(name="description", content="Описание страницы")

block content
	main.main
		section.s1
			.ibg.s1__ibg
				+picture(jpg, `${assets}s1-bg`)
			.container.s1__container
				.s1__circle
				h1.s1__title #[= lang[4][0]]#[br] #[span.text-primary= lang[4][1]]
				p.s1__subtitle!= lang[5]
				a.uk-button.btn-yellow.s1__btn(href="#callback" data-uk-toggle="")!= lang[6]
				.s1__items
					-
						var list = [
							{
								title: lang[7][0],
								subtitle: lang[8][0],
								icon: `armchair-light`
							},
							{
								title: lang[7][1],
								subtitle: lang[8][1],
								icon: `tr-wheel`
							},
							{
								title: lang[7][2],
								subtitle: lang[8][2],
								icon: `spray`
							},
							{
								title: lang[7][3],
								subtitle: lang[8][3],
								icon: `chair`
							}
						]
					each item, idx in list
						.s1__item(class=`s1__item-${idx + 1}`)
							+icon(item.icon)
							.s1__item-title= item.title
							.s1__item-subtitle= item.subtitle
				+picture(png, `${assets}s1-woman`)(class="s1-img-woman")
				a.s1__play.play-video.d-none.d-md-flex(href="#v-about" data-uk-toggle="")
					+icon('playbtn')
					.play-video__helper.play-video__helper--expanded= lang[9]

		section.s2
			.container
				.s2__items
					.s2__item.s2__item-1
						+icon('plant')
						p.s2__item-text #[= lang[10][0]]#[br.d-none.d-md-inline.d-lg-none] #[= lang[10][1]]#[br] #[span.d-md-none.d-lg-inline= lang[10][2]]
					.s2__item.s2__item-2
						+icon('study')
						p.s2__item-text #[= lang[11][0]]#[br] #[= lang[11][1]]
					.s2__item.s2__item-3
						+icon('payment')
						p.s2__item-text #[= lang[12][0]]#[br] #[= lang[12][1]]
		section.s3
			.container
				.s3__sec-1
					h2= lang[13]
					.s3__items
						a(href="#quiz-1" data-uk-toggle).s3__item.s3__item-1
							.ibg.s3__item-ibg
								+picture(jpg, `${assets}s3-i1`, 'n', '')
							p.s3__item-title!= lang[14][0][0]
							ul.s3__item-list
								li.s3__item-list-item= lang[14][0][1]
								li.s3__item-list-item= lang[14][0][2]
								li.s3__item-list-item= lang[14][0][3]
								li.s3__item-list-item= lang[14][0][4]
								li.s3__item-list-item= lang[14][0][5]
						a(href="#quiz-2" data-uk-toggle).s3__item.s3__item-2
							.ibg.s3__item-ibg
								+picture(jpg, `${assets}s3-i2`, 'n', '')
							p.s3__item-title!= lang[14][1][0]
							ul.s3__item-list
								li.s3__item-list-item= lang[14][1][1]
								li.s3__item-list-item= lang[14][1][2]
								li.s3__item-list-item= lang[14][1][3]
						a(href="#quiz-3" data-uk-toggle).s3__item.s3__item-3
							.ibg.s3__item-ibg
								+picture(jpg, `${assets}s3-i3`, 'n', '')
							p.s3__item-title!= lang[14][2][0]
							ul.s3__item-list
								li.s3__item-list-item= lang[14][2][1]
								li.s3__item-list-item= lang[14][2][2]
								li.s3__item-list-item= lang[14][2][3]
								li.s3__item-list-item= lang[14][2][4]
						a(href="#quiz-4" data-uk-toggle).s3__item.s3__item-4
							.ibg.s3__item-ibg
								+picture(jpg, `${assets}s3-i4`, 'n', '')
							p.s3__item-title!= lang[14][3][0]
							ul.s3__item-list
								li.s3__item-list-item!= lang[14][3][1]
								li.s3__item-list-item!= lang[14][3][2]

				.s3__sec-2
					h3= lang[15]
					.s3__manager-wrap
						.s3__manager-photo.ibg
							+picture(png, `${assets}s3-face`, 'n', '')
						.s3__manager-info
							p.s3__manager-name= lang[16]
							p.s3__manager-pos= lang[17]
							a.s3__manager-btn.uk-button.btn-solid-primary(href="#callback" data-uk-toggle)= lang[18]
							a.s3__manager-tel(href=phoneLink)= phoneNumber
		section.s4
			.container
				h2= lang[19]
				p.s4__subtitle!= lang[20]
				ul.s4__list
					-
						var list = [
							{
								icon: `study`,
								text: lang[21][0]
							},
							{
								icon: `plant`,
								text: lang[21][1]
							},
							{
								icon: `clean-time`,
								text: lang[21][2]
							},
							{
								icon: `messaging`,
								text: lang[21][3]
							}
						]
					each item, idx in list
						li.s4__list-item(class=`s4__list-item-${idx + 1}`)
							.s4__list-icon
								+icon(item.icon)
							p!= item.text
				.s4__warning
					.s4__warning-icon
						+icon('warn', 'white')
					p.s4__warning-text= lang[22]
				.s4__img-circle
				+picture(png, `${assets}s4-manager`, 'sm', 'm')(class="s4__img-manager")
				.s4__whoisshe
					.s4__whoisshe-name= lang[23]
					.s4__whoisshe-pos= lang[24]
				.s4__certificate-wrap
					.uk-slider-container
						ul.uk-slider-items.s4__certificate-items
							- for (var i = 1; i <= 4; i++)
								li.s4__certificate-item(class=`s4__certificate-item-${i}`)
									+picture(png, `${assets}s4-certificate-i${i}`, 'n', '')
					+slider-dots()(class="d-md-none")
		section.s5
			.ibg.s5__ibg
				+picture(jpg, `${assets}s5-bg`, 'sml', '')
			.container
				h2= lang[25]
				.s5__team-wrap
					- for(var x = 1; x <= 5; x++)
						.s5__team-circle(class=`s5__team-circle-${x}`)
					.uk-slider-container
						ul.uk-slider-items.s5__team-items
							-
								var list = [
									{
										name: lang[26][0][0],
										position: lang[26][0][1]
									},
									{
										name: lang[26][1][0],
										position: lang[26][1][1]
									},
									{
										name: lang[26][2][0],
										position: lang[26][2][1]
									},
									{
										name: lang[26][3][0],
										position: lang[26][3][1]
									},
									{
										name: lang[26][4][0],
										position: lang[26][4][1]
									}
								]
							each item, idx in list
								li.s5__team-item(class=`s5__team-item-${idx + 1}`)
									.s5__team-img.ibg
										+picture(jpg, `${assets}s5-person-i${idx + 1}`, 'n', '')
									.s5__team-descr
										p.s5__team-name= item.name
										p.s5__team-pos!= item.position
					+slider-dots()(class="slidedots--white d-lg-none")
		section.s6
			.container
				h2= lang[27]
				p.s6__subtitle= lang[28]
				+picture(png, `${assets}s6-woman`, 'n', '')(class="s6__img-woman")
				.s6__oncall
					p.s6__oncall-title= lang[29]
					p.s6__oncall-text= lang[30]
				ul.s6__items
					li.s6__item.s6__item-1
						.s6__item-img.ibg
							+picture(jpg, `${assets}s6-i1`, 'n', '')
						p.s6__item-descr!= lang[31][0]
					li.s6__item.s6__item-2
						.s6__item-img.ibg
							+picture(jpg, `${assets}s6-i2`, 'n', '')
						p.s6__item-descr!= lang[31][1]
					li.s6__item.s6__item-3
						.s6__item-img.ibg
							+picture(jpg, `${assets}s6-i3`, 'n', '')
						p.s6__item-descr!= lang[31][2]
					li.s6__item.s6__item-4
						.s6__item-img.ibg
							+picture(jpg, `${assets}s6-i4`, 'n', '')
						p.s6__item-descr!= lang[31][3]
				.s6__notice #[b!= lang[32][0]] #[!= lang[32][1]] #[b!= lang[32][2]] 
				.s6__adv
					+picture(png, `${assets}s6-adv`, 'sml', '')(class="s6__adv-img")
					p.s6__adv-title #[!= lang[33][0]] #[span.d-none.d-lg-inline.text-primary!= lang[33][1]] #[span.d-none.d-lg-inline!= lang[33][2]]
					p.s6__adv-subtitle #[span.text-primary!= lang[33][1]] #[!= lang[33][2]]
					p.s6__adv-p1= lang[34]
					p.s6__adv-p2= lang[35]
					a.s6__adv-btn.uk-button.btn-solid-primary(href="#rig" data-uk-toggle)= lang[36]
					.s6__adv-warn
						p.s6__adv-warn-text #[= lang[37][0]]#[br] #[!= lang[37][1]]
						+icon('warn', 'white')
		section.s7
			.ibg.s7__ibg
				+picture(jpg, `${assets}s7-bg`, 'sl', '')
			.container
				h2= lang[38]
				-
					var list = [
						{
							icon: 'armchair',
							text: lang[39][0],
							count: 3
						},
						{
							icon: 'sofa',
							text: lang[39][1],
							count: 6
						},
						{
							icon: 'carpet',
							text: lang[39][2],
							count: 1
						},
						{
							icon: 'sink',
							text: lang[39][3],
							count: 1
						},
						{
							icon: 'fireplace',
							text: lang[39][4],
							count: 2
						},
						{
							icon: 'spray-right',
							text: lang[39][4],
							count: 1
						}
					]
				.s7__slideshow-wrap
					each item, idx in list
						.s7__slideshow(class=`s7__slideshow-${idx + 1} ${idx == 1 ? 'active' : ''}`)
							.s7__slideshowdrag
								.s7__slideshowdrag-grab
									+icon('drag', 'white')
							ul.s7__slideshow-items.uk-slideshow-items
								
								- for(var i = 1; i <= item.count; i++)
									li.s7__slideshow-item
										.ibg.s7__slideshow-a
											+picture(jpg, `${assets}preview/${idx + 1}/i${i}-a`, 'n', '')
										.s7__slideshow__hide-block
											.ibg.s7__slideshow-b
												+picture(jpg, `${assets}preview/${idx + 1}/i${i}-b`, 'n', '')


							+slideshow-nav()(class="slidenav--rounded")
				.s7__tab
					.uk-slider-container
						ul.s7__tab-items.uk-slider-items
							each item, idx in list
								li.s7__tab-item(class=`${idx == 1 ? 'active' : ''}`)
									a(href="#")
										.s7__tab-icon(class=`s7__tab-icon-${idx + 1}`)
											+icon(item.icon)
										.s7__tab-divider
										p.s7__tab-text= item.text
					+slider-nav()(class="slidenav--outline")
		section.s8 
			.container 
				.s8__widget 
					h2= lang[40]
					.s8__slideshow

						ul.s8__slideshow-items.uk-slideshow-items
							
							each item, idx in lang[41]
								li.s8__slideshow-item
									.s8__slideshow-image.ibg 
										+picture(jpg, `${assets}s8-i${idx+1}`, 'n', 'n')
										.s8__slideshow-title.d-lg-none= item.title
									ul.s8__slideshow-list 
										.s8__slideshow-title.d-none.d-lg-block= item.title
										each point in item.points 
											li.s8__slideshow-listitem!= point
						+slideshow-dots()(class="d-lg-none")
						nav.s8__slideshow-nav.d-none.d-lg-flex
							each item, idx in lang[41]
								a.s8__slideshow-navitem(href="#" class=`${idx == 0 ? 'active' : ''}`)
									span(data-text=item.title)!= item.title
		section.s9
			.s9__img-bg.ibg 
				+picture(jpg, `${assets}s9-bg`, 'sml', '')
			.container
				h2 #[=lang[42][0]] #[span.text-yellow!=lang[42][1]]
				.s9__slider(data-uk-slider)
					.uk-slider-container 
						ul.s9__slider-items.uk-slider-items 
							- for(var i = 1; i <= 3; i++)
								li.s9__slider-item 
									.s9__slider-image.ibg 
										+picture(jpg, `${assets}s9-i${i}`, 'n', '')
									a.s9__slider-play.play-video(href="#v" data-uk-toggle="")
										+icon('playbtn-reverse')
										.play-video__helper.play-video__helper!=lang[43]
					+slider-dots(class="slidedots--yellow d-lg-none")
					+slider-nav(class="d-none d-lg-flex")
		section.s10 
			.container 
				h2=lang[44]
				ul.s10__acc(data-uk-accordion)
					li.s10__acc-item.s10__acc-item--first.uk-open 
						a.s10__acc-title.uk-accordion-title(href="#")= lang[45][0]
							+icon('angle-down', '#2B3034')(class="d-none d-md-block")
						.s10__acc-content.uk-accordion-content
							p= lang[46]
							h5= lang[47]
							.s10__acc-content__certificates 
								+picture(jpg, `${assets}s10-cert-i1`, 'n', 'n')(class="s10__acc-content__img")
								+picture(jpg, `${assets}s10-cert-i2`, 'n', 'n')(class="s10__acc-content__img")
								+picture(jpg, `${assets}s10-cert-i3`, 'n', 'n')(class="s10__acc-content__img")
								+picture(jpg, `${assets}s10-cert-i4`, 'n', 'n')(class="s10__acc-content__img")
					li.s10__acc-item
						a.s10__acc-title.uk-accordion-title(href="#")= lang[45][1]
							+icon('angle-down', '#2B3034')(class="d-none d-md-block")
						.s10__acc-content.uk-accordion-content
							p= lang[46]
					li.s10__acc-item
						a.s10__acc-title.uk-accordion-title(href="#")= lang[45][2]
							+icon('angle-down', '#2B3034')(class="d-none d-md-block")
						.s10__acc-content.uk-accordion-content
							p= lang[46]
					li.s10__acc-item
						a.s10__acc-title.uk-accordion-title(href="#")= lang[45][3]
							+icon('angle-down', '#2B3034')(class="d-none d-md-block")
						.s10__acc-content.uk-accordion-content
							p= lang[46]
					li.s10__acc-item.s10__acc-item--last
						a.s10__acc-title.uk-accordion-title(href="#")= lang[45][4]
							+icon('angle-down', '#2B3034')(class="d-none d-md-block")
						.s10__acc-content.uk-accordion-content
							p= lang[46]
				.s10__manager.d-none.d-lg-flex 
					+picture(png, `${assets}s10-woman`, 'l', 'l')
					p.s10__manager-name= lang[16]
					p.s10__manager-pos= lang[17]
					a.s10__manager-btn.uk-button.btn-solid-primary(href="#callback" data-uk-toggle)= lang[18]
		section.s11 
			.s11__ibg.ibg
				+picture(jpg, `${assets}s11-bg`, 'ml', '')
			.container 
				.s11__map
					.s11__map-downloading
						.loadingio-spinner-rolling-hz3sd853mcv
							.ldio-ekx347c8429
								div
					.s11__map-script



